	"description": "A simple wrapper class for Promise class, which add multiple advanced functionality to the default promise like Aborting, data binding, status checking.",
	"source": "src/index.js",
	"main": "dist/main.js",
	"module": "dist/module.js",
	"targets": {
		"main": {
			"optimize": true
		},
		"module": {
			"optimize": true
		}
	},
